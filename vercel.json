{
  "functions": {
    "api/main.py": {
      "runtime": "python3.12"
    }
  },
  "rewrites": [
    {
      "source": "/api/spotify",
      "destination": "/api/main.py"
    },
    {
      "source": "/api/spotify/(.*)",
      "destination": "/api/main.py"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/apchapcomputing/apchapcomputing"
    }
  ]
}
